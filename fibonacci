#include <stdio.h>

int fib1(int n) {
  // iteration method
  
  if ( n < 2 ) return n;
  int pre = 0;
  int cur = 1;
  for ( int i = 2; i < n+1; ++i) {
    int new = pre + cur;
    pre = cur;
    cur = new;
  } 
  return cur;
}

int fib2(int n) {
  if (n < 2) return n;
  return fib2(n-1) + fib2(n-2);
}

int main() {
  int r = 0;
  
  r = fib1(10);
  printf("%d ", r);
  r = fib2(10);
  printf("%d ", r);
  return 0;
}
