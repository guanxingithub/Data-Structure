#include <stdio.h>

int fib1(int n) {
  // iteration method
  
  if ( n < 2 ) return n;
  int pre = 0;
  int cur = 1;
  for ( int i = 2; i < n+1; ++i) {
    int new = pre + cur;
    pre = cur;
    cur = new;
  } 
  return cur;
}

int fib2(int n) {
  if (n < 2) return n;
  return fib2(n-1) + fib2(n-2);
}

int F[10] = {-1};
int fib3(int n) {
  if (n < 2) {
    // memorize the result;
    F[n] = n; // memorization
    return F[n];
  } 
  if ( F[n-1] == -1 ) {
    F[n-1] = fib3(n-1);
  }
  if ( F[n-2] == -1 ) {
    F[n-2] = fib3(n-2);
  }
  F[n] = F[n-1] + F[n-2]; // memorization
  return F[n];
}


int main() {
  int r = 0;
  
  r = fib1(10);
  printf("%d ", r);
  r = fib2(10);
  printf("%d ", r);
  return 0;
}
