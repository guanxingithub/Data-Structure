#include<stdio.h>
#include<stdlib.h>

int findMissing(struct Array arr)
{
  int sum = 0;
  for ( i = 0;i < arr.length; i++) {
    sum += arr.A[i];
  }
  int s = A[length-1] * ( A[length-1] + 1) /2
  return s - sum;
}

// c++ code
vector<int> findMissing1(vector<int> list)
{
  int diff = list[0];
  vector<int> M;
  for ( int i = 0; i < list.size(); i++) {
    if ( list[i] - i != diff ) {
      while ( diff != list[i] -i ) {
        M.push_back(i + diff);
      }
    }
  }
  return M;
}
